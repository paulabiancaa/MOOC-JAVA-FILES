
import java.util.Scanner;

public class Mooc {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        String word;
        int i = 0;
        while(true){
            word = scan.nextLine();
            
            if(word.equals("end")){
                break;
            }
            i++;
        }
        System.out.println(i);
    }
}

//doing parseInt or using Integer.valueOf will produce an number
